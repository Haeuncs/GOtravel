# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'
use_frameworks!

def available_pods
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'IQKeyboardManagerSwift'
    pod 'AnimatedTextInput'
    pod 'SnapKit'
    pod 'RxDataSources'
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'CenteredCollectionView'
    pod 'SwiftLint'
end

def travelDesignSystem_pods
  pod 'SnapKit'
end

target 'GOtravel' do
  available_pods
end

target 'TravelDesignSystem' do
  travelDesignSystem_pods
end
